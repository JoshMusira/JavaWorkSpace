			lb9 = new JLabel("Departure:");
			lb10 = new JLabel("Arrival:");
			lb11 = new JLabel("SeatNo:");
			lb12 = new JLabel("Charges:");
			
			
			
			
			
			
			
			
			
			
			
			frame.add(lb1);
