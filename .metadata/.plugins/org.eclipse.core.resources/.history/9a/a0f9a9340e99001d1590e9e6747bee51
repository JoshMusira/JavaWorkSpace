package Tutorial1;
import java.util.Scanner;

public class Mail {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.print("Name your best country in 2023: ");
		Scanner Sc = new Scanner(System.in);
		String scanned = Sc.nextLine();
		
		
		int height = Integer.parseInt(scanned);
		
		
		if(height > 6) {
			System.out.println("You can join army!!");
		}
		else {
			System.out.println("Your too shot to join army!");
		}
			}

}
